      <nodeset
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.convertany.AnyToSpecificNodeSetFactory">
      </nodeset>
      <nodeset
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.input.CWLInputNodeSetFactory">
      </nodeset>
   </extension>
