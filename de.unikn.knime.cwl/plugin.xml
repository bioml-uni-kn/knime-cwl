   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Auto-generated CWL nodes and utilities"
            icon="icons/missing.gif"
            level-id="cwl"
            locked="true"
            name="CWL"
            path="/">
      </category>
      <category
            description="Utility nodes for CWL execution"
            icon="icons/missing.gif"
            level-id="util"
            locked="true"
            name="Utilities"
            path="/cwl">
      </category>
      <category
            description="Data type conversion utilities"
            icon="icons/missing.gif"
            level-id="convert"
            locked="true"
            name="Conversion"
            path="/cwl/util">
      </category>
      <category
            description="Input nodes for CWL execution"
            icon="icons/missing.gif"
            level-id="input"
            locked="true"
            name="Inputs"
            path="/cwl/util">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/cwl/util"
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.createrecord.BuildRecordNodeFactory">
      </node>
      <node
            category-path="/cwl/util"
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.createarray.BuildArrayNodeFactory">
      </node>
      <node
            category-path="/cwl/util/convert"
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.porttojson.CWLPortToJSONNodeFactory">
      </node>
      <node
            category-path="/cwl/util/convert"
            deprecated="false"
            factory-class="de.unikn.knime.cwl.util.node.porttovariable.CWLPortToVariableNodeFactory">
      </node>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.workbench.ui.preferences"
            class="de.unikn.knime.cwl.preferences.CWLPreferencePage"
            id="org.knime.workbench.ui.preferences.cwl"
            name="CWL">
      </page>
   </extension>
   
</plugin>
